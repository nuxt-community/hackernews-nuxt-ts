{
  "name": "nuxt-ts-hn",
  "description": "Nuxt Typescript Hacker News",
  "version": "1.0.0",
  "author": "Evan You <yyx990803@gmail.com>",
  "contributors": [
    {
      "name": "Sebastien Chopin (@Atinux)"
    },
    {
      "name": "Alexandre Chopin (@alexchopin)"
    },
    {
      "name": "Pooya Parsa (@pi0)"
    },
    {
      "name": "HG(@husayt)"
    },
    {
      "name": "Kevin Marrec (@kevinmarrec)"
    }
  ],
  "private": true,
  "scripts": {
    "dev": "nuxt-ts dev",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "dev-spa": "nuxt-ts dev --spa",
    "build-spa": "nuxt-ts build --spa",
    "start-spa": "nuxt-ts start --spa",
    "tsc": "tsc --showConfig",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "testc": "tsc -p tsconfig.json",
    "lint": "tslint --project .",
    "lintfix": "tslint --fix --project .",
    "post-update": "yarn upgrade --latest"
  },
  "now": {
    "alias": "hn.nuxtjs.org"
  },
  "engines": {
    "node": ">=8.0"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.2.0",
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/component-cache": "1.1.5",
    "@nuxtjs/pwa": "3.0.0-beta.19",
    "apicache": "1.5.2",
    "express": "4.17.1",
    "nuxt": "^2.10.0",
    "vue-property-decorator": "^8.2.2"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.3.0",
    "@types/jest": "^24.0.18",
    "@vue/test-utils": "^1.0.0-beta.29",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "babel-core": "^6.26.3",
    "prettier": "^1.18.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^24.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "vue-jest": "^3.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts$",
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  }
}
